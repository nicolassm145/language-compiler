[3º exemplo: menu para direcionar a recepção de um banco]
FACA
  ESCREVA 'digite opcao:',
  ESCREVA '1= caixa, 2= contratos',
  LEIA op,
  SE op = 1 ENTAO
    ESCREVA '1 : guiches 7 a 15',
    op := 0,
  SENAO
    SE op = 2 ENTAO
      ESCREVA '2 : guiches 20 e 23',
      op := 0,
    FIM
  FIM
ENQUANTO 0 < op
